{
  "name": "hsl-shared-components",
  "version": "0.13.0-0",
  "description": "Shared components to be used in HSL projects. React and styled components.",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.0.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "eventemitter3": "^2.0.3",
    "jest": "^20.0.4",
    "prop-types": "^15.5.10",
    "react": "15.4.x",
    "react-dom": "^15.4.x",
    "react-native": "0.42.x",
    "react-native-scripts": "^0.0.22",
    "react-native-svg": "4.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-scripts": "^1.0.10",
    "react-test-renderer": "15.4.x",
    "react-transition-group": "^1.2.0",
    "redux": "^3.7.1",
    "skpm": "^0.10.5",
    "styled-components": "^2.1.1"
  },
  "main": "lib/",
  "skpm": {
    "main": "hsl-shared-components.sketchplugin",
    "manifest": "./skpm-manifest.json"
  },
  "scripts": {
    "prepublish": "babel src/ --out-dir lib/ --ignore .test.js",
    "build:watch": "babel src/ --out-dir lib/ --watch --ignore .test.js",
    "test": "jest",
    "es-lint": "eslint .",
    "sketch:build": "skpm build",
    "sketch:watch": "skpm build --watch",
    "sketch:render": "skpm build --watch --run",
    "sketch:render:once": "skpm build --run",
    "sketch:link-plugin": "skpm link"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "polished": "^1.2.1",
    "react-primitives": "ktj/primitives-build",
    "react-primitives-svg": "ktj/primitives-svg-build",
    "react-sketchapp": "^0.12.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-native-svg": "*",
    "react-transition-group": "^1.2.0",
    "styled-components": "*"
  }
}
