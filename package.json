{
  "name": "hsl-shared-components",
  "version": "0.9.0",
  "description": "Shared components to be used in HSL projects. React and styled components.",
  "devDependencies": {
    "@storybook/addon-actions": "3.1.2",
    "@storybook/addon-knobs": "3.1.2",
    "@storybook/addon-links": "3.1.2",
    "@storybook/addon-storyshots": "3.1.2",
    "ktj-storyshots": "3.1.2",
    "@storybook/react": "3.1.2",
    "@storybook/react-native": "3.1.2",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.0.2",
    "concurrently": "^3.5.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "jest": "^20.0.4",
    "jest-expo": "^0.3.0",
    "react-dom": "^15.4.x",
    "react-native-scripts": "^0.0.22",
    "react-native-storybook-loader": "^1.4.1",
    "react-test-renderer": "15.4.x",
    "storybook-addon-jsx": "^4.1.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "build:babel": "babel src/ --out-dir lib/ --ignore test.js",
    "build": "yarn run build:babel && ./scripts/publish.sh",
    "lint:tight": "esw src/ --color --max-warnings 0",
    "lint:watch": "esw src/ -w --color",
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest --testPathPattern=stories_web && jest --testPathPattern=stories_native --config=native.jest.config.json",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007 -c storybook_native",
    "storybook:web": "start-storybook -c storybook_web -s ./static -p 6006",
    "build-storybook": "build-storybook -c storybook_web -s ./static",
    "deploy-storybook": "storybook-to-ghpages",
    "test-update": "jest --testPathPattern=stories_web -u && jest --testPathPattern=stories_native --config=native.jest.config.json -u",
    "es-lint": "eslint ."
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "eventemitter3": "^2.0.3",
    "expo": "15.x.x",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "polished": "^1.2.1",
    "prop-types": "^15.5.10",
    "react": "15.4.x",
    "react-native": "0.42.x",
    "react-primitives": "ktj/primitives-build",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.2",
    "react-scripts": "^1.0.10",
    "react-transition-group": "^1.2.0",
    "redux": "^3.7.1",
    "styled-components": "^2.1.1"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./stories_native"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook_native/storyLoader.js"
    }
  }
}
