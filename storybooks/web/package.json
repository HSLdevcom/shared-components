{
  "name": "web-storybook",
  "private": true,
  "version": "0.12.0",
  "description": "Shared components to be used in HSL projects. React and styled components.",
  "devDependencies": {
    "@storybook/addon-actions": "3.1.2",
    "@storybook/addon-knobs": "3.1.2",
    "@storybook/addon-links": "3.1.2",
    "@storybook/addon-storyshots": "3.1.2",
    "@storybook/react": "3.1.2",
    "@storybook/storybook-deployer": "^2.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.0.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-node": "^0.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-watch": "^2.1.14",
    "eventemitter3": "^2.0.3",
    "hsl-shared-components": "^0.12.0",
    "jest": "^20.0.4",
    "prop-types": "^15.5.10",
    "react": "15.4.x",
    "react-dom": "^15.4.x",
    "react-primitives": "ktj/primitives-build",
    "react-redux": "^5.0.5",
    "react-scripts": "^1.0.10",
    "react-test-renderer": "15.4.x",
    "storybook-addon-jsx": "^4.1.1",
    "styled-components": "^2.1.1"
  },
  "scripts": {
    "build:babel": "babel src/ --out-dir lib/ --ignore .test.js",
    "build": "yarn run build:babel && ./scripts/publish.sh",
    "lint:tight": "esw src/ --color --max-warnings 0",
    "lint:watch": "esw src/ -w --color",
    "test": "jest --testPathPattern=stories",
    "storybook": "start-storybook -c storybook -p 6006",
    "build-storybook": "build-storybook -c storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "test-update": "jest --testPathPattern=stories -u",
    "es-lint": "eslint ."
  }
}
